<p id="notice"><%= notice %></p>

<h1>Listing Myb Evl Creations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Author</th>
      <th>Target user</th>
      <th>Serial</th>
      <th>Schedule</th>
      <th>Eval time min</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myb_evl_creations.each do |myb_evl_creation| %>
      <tr>
        <td><%= myb_evl_creation.Name %></td>
        <td><%= myb_evl_creation.Author_id %></td>
        <td><%= myb_evl_creation.Target_User %></td>
        <td><%= myb_evl_creation.Serial %></td>
        <td><%= myb_evl_creation.Schedule %></td>
        <td><%= myb_evl_creation.Eval_Time_Min %></td>
        <td><%= link_to 'Show', myb_evl_creation %></td>
        <td><%= link_to 'Edit', edit_myb_evl_creation_path(myb_evl_creation) %></td>
        <td><%= link_to 'Destroy', myb_evl_creation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

