<p id="notice"><%= notice %></p>

<h1>Listing Myb Evl Quests</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Question</th>
      <th>Answer</th>
      <th>Evl</th>
      <th>Serial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myb_evl_quests.each do |myb_evl_quest| %>
      <tr>
        <td><%= myb_evl_quest.Type %></td>
        <td><%= myb_evl_quest.Question %></td>
        <td><%= myb_evl_quest.Answer %></td>
        <td><%= myb_evl_quest.Evl_id %></td>
        <td><%= myb_evl_quest.Serial %></td>
        <td><%= link_to 'Show', myb_evl_quest %></td>
        <td><%= link_to 'Edit', edit_myb_evl_quest_path(myb_evl_quest) %></td>
        <td><%= link_to 'Destroy', myb_evl_quest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myb evl quest', new_myb_evl_quest_path %>
